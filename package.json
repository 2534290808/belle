{
  "name": "belle",
  "version": "0.0.9",
  "description": "React Components with great UX",
  "author": {
    "name": "Nik Graf",
    "email": "nik@nikgraf.com",
    "url": "http://www.nikgraf.com"
  },
  "main": "lib/index.js",
  "keywords": [
    "browser",
